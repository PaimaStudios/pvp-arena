    p1_stats = [p1_stats[0], calc_stats(p1_heroes[1].value), calc_stats(p1_heroes[2].value)];
    game_state = GAME_STATE.p2_selecting_last_hero;


    base_damage_cache_p1_1_0 = ((100 - p2_stats[0].crush_def) * p1_stats[1].crush_dmg + (100 - p2_stats[0].pierce_def) * p1_stats[1].pierce_dmg + (40 + p2_stats[0].weight - p1_stats[1].weight) * p1_stats[1].dex_bonus) as Uint<32>;
